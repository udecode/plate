{
  "name": "@platejs/utils",
  "version": "50.3.8",
  "description": "Plate utils",
  "keywords": [
    "plate",
    "plugin",
    "slate"
  ],
  "homepage": "https://platejs.org",
  "bugs": {
    "url": "https://github.com/udecode/plate/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/udecode/plate.git",
    "directory": "packages/utils"
  },
  "license": "MIT",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "module": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./react": {
      "types": "./dist/react/index.d.ts",
      "import": "./dist/react/index.mjs",
      "module": "./dist/react/index.mjs",
      "require": "./dist/react/index.js"
    }
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "brl": "yarn p:brl",
    "build": "yarn p:build",
    "build:watch": "yarn p:build:watch",
    "clean": "yarn p:clean",
    "lint": "yarn p:lint",
    "lint:fix": "yarn p:lint:fix",
    "test": "yarn p:test",
    "test:watch": "yarn p:test:watch",
    "typecheck": "yarn p:typecheck"
  },
  "dependencies": {
    "@platejs/core": "50.3.8",
    "@platejs/slate": "49.2.21",
    "@udecode/react-utils": "49.0.15",
    "@udecode/utils": "47.2.7",
    "clsx": "^2.1.1",
    "lodash": "^4.17.21"
  },
  "publishConfig": {
    "access": "public"
  }
}
