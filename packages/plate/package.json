    "@udecode/react-utils": "49.0.15",
    "@udecode/utils": "47.2.7"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
