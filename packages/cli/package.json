{
  "name": "@udecode/plate-ui",
  "version": "30.6.2",
  "description": "Add Plate components to your apps.",
  "license": "MIT",
  "homepage": "https://platejs.org",
  "repository": {
    "type": "git",
    "url": "https://github.com/udecode/plate.git",
    "directory": "packages/cli"
  },
  "bugs": {
    "url": "https://github.com/udecode/plate/issues"
  },
  "type": "module",
  "sideEffects": false,
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "module": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "bin": "./dist/index.js",
  "scripts": {
    "build": "yarn tsup",
    "build:watch": "yarn tsup --watch",
    "clean": "yarn p:clean && rimraf components",
    "dev": "COMPONENTS_REGISTRY_URL=http://localhost:3000 node dist/index.js",
    "lint": "yarn p:lint",
    "lint:fix": "yarn p:lint:fix",
    "start": "node dist/index.js",
    "test": "vitest run",
    "test:watch": "yarn p:test:watch",
    "typecheck": "yarn p:typecheck"
  },
  "dependencies": {
    "@antfu/ni": "^0.21.12",
    "chalk": "5.3.0",
    "commander": "^10.0.1",
    "cosmiconfig": "^8.3.6",
    "diff": "^5.2.0",
    "execa": "^7.2.0",
    "fast-glob": "^3.3.2",
    "fs-extra": "^11.2.0",
    "https-proxy-agent": "^6.2.1",
    "lodash.template": "^4.5.0",
    "node-fetch": "^3.3.2",
    "ora": "^6.3.1",
    "prompts": "^2.4.2",
    "ts-morph": "^18.0.0",
    "tsconfig-paths": "^4.2.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/diff": "^5.0.9",
    "@types/fs-extra": "^11.0.4",
    "@types/prompts": "^2.4.9",
    "tsup": "^8.0.2",
    "type-fest": "^4.13.1",
    "vitest": "^0.32.4"
  },
  "keywords": [
    "components",
    "plate",
    "slate",
    "ui",
    "tailwind",
    "radix-ui",
    "shadcn"
  ],
  "publishConfig": {
    "access": "public"
  }
}
