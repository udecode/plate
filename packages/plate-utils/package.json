{
  "name": "@udecode/plate-utils",
  "version": "41.0.2",
  "description": "Plate utils",
  "keywords": [
    "plate",
    "plugin",
    "slate"
  ],
  "homepage": "https://platejs.org",
  "bugs": {
    "url": "https://github.com/udecode/plate/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/udecode/plate.git",
    "directory": "packages/utils"
  },
  "license": "MIT",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "module": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./react": {
      "types": "./dist/react/index.d.ts",
      "import": "./dist/react/index.mjs",
      "module": "./dist/react/index.mjs",
      "require": "./dist/react/index.js"
    }
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "brl": "INIT_CWD=\"$PWD\" pnpm -w run p:brl",
    "build": "INIT_CWD=\"$PWD\" pnpm -w run p:build",
    "build:watch": "INIT_CWD=\"$PWD\" pnpm -w run p:build:watch",
    "clean": "INIT_CWD=\"$PWD\" pnpm -w run p:clean",
    "lint": "INIT_CWD=\"$PWD\" pnpm -w run p:lint",
    "lint:fix": "INIT_CWD=\"$PWD\" pnpm -w run p:lint:fix",
    "test": "INIT_CWD=\"$PWD\" pnpm -w run p:test",
    "test:watch": "INIT_CWD=\"$PWD\" pnpm -w run p:test:watch",
    "typecheck": "INIT_CWD=\"$PWD\" pnpm -w run p:typecheck"
  },
  "dependencies": {
    "@udecode/plate-core": "41.0.5",
    "@udecode/react-utils": "40.2.8",
    "@udecode/slate": "41.0.0",
    "@udecode/slate-react": "41.0.5",
    "@udecode/slate-utils": "41.0.0",
    "@udecode/utils": "37.0.0",
    "clsx": "^2.1.1",
    "lodash": "^4.17.21"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0",
    "slate": ">=0.112.0",
    "slate-history": ">=0.93.0",
    "slate-hyperscript": ">=0.66.0",
    "slate-react": ">=0.110.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
