{
  "name": "depset",
  "version": "0.1.1",
  "description": "Synchronize dependencies in package.json",
  "keywords": [
    "dependencies",
    "package.json",
    "sync",
    "upgrade",
    "version",
    "yarn",
    "pnpm",
    "npm",
    "bun"
  ],
  "homepage": "https://github.com/udecode/plate/packages/udecode/depset/README.md",
  "bugs": {
    "url": "https://github.com/udecode/plate/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/udecode/plate.git",
    "directory": "packages/udecode/depset"
  },
  "license": "MIT",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "bin": "./dist/index.js",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "build": "tsup",
    "build:watch": "tsup --watch",
    "lint": "biome check .",
    "lint:fix": "biome check . --fix",
    "clean": "rimraf dist && jest --clear-cache",
    "start": "node dist/index.js",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@antfu/ni": "24.3.0",
    "commander": "14.0.0",
    "kleur": "4.1.5",
    "ora": "8.2.0",
    "prompts": "2.4.2",
    "zod": "4.1.12"
  },
  "devDependencies": {
    "@types/prompts": "2.4.9",
    "tsup": "8.5.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
