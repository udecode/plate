{
  "packages": [
    "packages/*"
  ],
  "version": "0.58.14",
  "npmClient": "yarn",
  "useWorkspaces": true,
  "includeMergedTags": true,
  "ignoreChanges": [
    "**test**",
    "**.spec**",
    "**/*.md",
    "**/*stories*",
    "**template**",
    "**fixture**"
  ],
  "command": {
    "publish": {
      "registry": "https://npm.pkg.github.com"
    }
  }
}
