{
  "private": true,
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "yarn workspaces foreach -pt --exclude 'examples*' run build ",
    "build:all": "yarn && yarn build && yarn build:docs",
    "build:docs": "cd docs && yarn && yarn build",
    "clean": "yarn workspaces foreach -p run prebuild && yarn workspace @udecode/plate-test-utils build",
    "clean:modules": "rimraf '**/node_modules' yarn.lock",
    "codegen": "yarn plop --plopfile ./config/plopfile.js",
    "cti": "yarn workspaces foreach -p --exclude 'examples*' run cti",
    "cti:clean": "yarn workspaces foreach -p --exclude 'examples*' run cti clean ./src",
    "dev": "yarn workspace examples-next dev",
    "docs": "cd docs && yarn && yarn start",
    "postinstall": "patch-package",
    "internal:release:next": "yarn prerelease && yarn changeset publish --tag next",
    "lint": "eslint \"packages/**/src/**/*.{js,jsx,ts,tsx}\"",
    "prerelease": "yarn build && yarn test && yarn lint",
    "test": "jest",
    "test:cov": "yarn test --coverage",
    "test:covw": "yarn test:cov --watch",
    "test:covwa": "yarn test:cov --watchAll",
    "test:w": "yarn test --watch",
    "test:wa": "yarn test --watchAll",
    "typedoc": "npx typedoc --tsconfig packages/plate/tsconfig.json --options config/typedoc.json"
  },
  "workspaces": [
    "packages/**",
    "examples",
    "examples/apps/**"
  ],
