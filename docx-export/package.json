{
  "name": "@platejs/docx-export",
  "version": "1.0.0",
  "description": "DOCX export plugin for Plate.js - Export rich text editor content to Microsoft Word format",
  "keywords": [
    "plate",
    "platejs",
    "slate",
    "editor",
    "rich-text",
    "docx",
    "word",
    "export",
    "microsoft-word",
    "document"
  ],
  "license": "MIT",
  "author": "Plate.js Contributors",
  "repository": {
    "type": "git",
    "url": "https://github.com/udecode/plate.git",
    "directory": "packages/docx-export"
  },
  "bugs": {
    "url": "https://github.com/udecode/plate/issues"
  },
  "homepage": "https://platejs.org/docs/docx-export",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "src",
    "README.md",
    "CHANGELOG.md"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint src --ext .ts,.tsx",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "lucide-react": ">=0.400.0",
    "platejs": ">=50.0.0",
    "react": ">=19.0.0",
    "react-dom": ">=19.0.0",
    "sonner": ">=1.4.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": false
    },
    "react-dom": {
      "optional": false
    }
  },
  "dependencies": {
    "@oozcitak/dom": "1.15.6",
    "@oozcitak/util": "8.3.4",
    "color-name": "^1.1.4",
    "html-entities": "^2.3.3",
    "html-to-vdom": "^0.7.0",
    "image-size": "^1.0.0",
    "image-to-base64": "^2.2.0",
    "jszip": "^3.7.1",
    "lodash": "^4.17.21",
    "mime-types": "^2.1.35",
    "nanoid": "^3.1.25",
    "virtual-dom": "^2.1.1",
    "xmlbuilder2": "2.1.2"
  },
  "devDependencies": {
    "@types/react": "^19.0.0",
    "jsdom": "^27.3.0",
    "lucide-react": "^0.400.0",
    "platejs": "^52.0.0",
    "react": "^19.0.0",
    "react-compiler-runtime": "^1.0.0",
    "react-dom": "^19.0.0",
    "sonner": "^1.4.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "resolutions": {
    "use-context-selector": "2.0.0"
  },
  "overrides": {
    "use-context-selector": "2.0.0"
  },
  "sideEffects": false
}
